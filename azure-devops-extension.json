{
  "manifestVersion": 1,
  "id": "samples",
  "publisher": "zhengqiang",
  "version": "1.0.12",
  "name": "Extension Sample",
  "description": "Azure DevOps extension sample",
  "categories": ["Azure Pipelines"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": ["vso.work_full"],
  "icons": {
    "default": "logo.png"
  },
  "content": {
